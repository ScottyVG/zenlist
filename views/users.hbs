        <a href="/logout" class="btn">Logout</a>
    </div>

    <div class="row">
        <div class="col offset-s2 s8">
            <div class="center-align">
                <h3><span class=""></span>Local user proto</h3>
                <table class="striped centerd responsive-table">
                    <th>Email</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    {{#if user}}
                        <tr>
                            <td>{{user.email}}</td>
                            <td>{{user.firstName}}</td>
                            <td>{{user.lastName}}</td>
                        </tr>
                    {{/if}}
                </table>
            </div>
        </div>
    </div>
</div>
