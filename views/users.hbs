<div class="container">
    <div class="center-align">
        <h1><span class=""></span> Profile Page Proto</h1>
        <a href="/logout" class="btn">Logout</a>
    </div>

    <div class="row">
        <div class="col offset-s2 s8">
            <div class="center-align">
                <h3><span class=""></span>Local user proto</h3>
                <table class="striped centerd responsive-table">
                    <th>Email</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    {{#if user}}
                        <tr>
                            <td>{{user.email}}</td>
                            <td>{{user.firstName}}</td>
                            <td>{{user.lastName}}</td>
                        </tr>
                    {{/if}}
                </table>
            </div>
        </div>
    </div>
</div>
<hr>
<div class="container">
    <div class="center-align">
        <h3>create lists proto</h3>
    </div>
    <div class="row">
        <div class="col s6">
            <div class="row input-field">
                <form class="input-field" action="/lists" method="post">
                    <div class="input-field col s12">
                        <input placeholder="" id="title" type="text" name="title" class="form-control">
                        <label for="title">title</label>
                    </div>
                    <div class="input-field col s12">
                        <textarea name="description" id="description" class="materialize-textarea"></textarea>
                        <label for="description">description</label>
                    </div>
                    <div class="input-field col s12">
                        <p class="range-field">
                            <input name="priorityLevel" type="range" id="priorityLevel" min="1" max="5" />
                        </p>
                    </div>
                    <!-- <div class=""> -->
                    <button type="submit" class="btn">Create list</button>
                    <!-- </div> -->
                </form>
            </div>
        </div>
    </div>
</div>
<hr>
<div class="container">
    <div class="center-align">
        <div class="row input-field">
            <form class="" action="/tasks" method="post">
                <div class="input-field col s12">
                    <input placeholder="" id="title" type="text" name="title" class="form-control">
                    <label for="title">title</label>
                </div>
                <div class="input-field col s12">
                    <textarea name="task" id="task" class="materialize-textarea"></textarea>
                    <label for="description">task</label>
                </div>
                <button type="submit" class="btn">Create task</button>
            </form>
        </div>
    </div>
</div>
<hr>
<div class="container">
    <div class="center-align">
        <h3>edit lists proto</h3>
    </div>
    <div class="row">
        <div class="col s6">
            <div class="row input-field">
                <form class="input-field" action="/lists" method="patch">
                  <div class="input-field col s12">
                      <input placeholder="" id="titleToEdit" type="text" name="titleToEdit" class="form-control">
                      <label for="titleToEdit">titleToEdit</label>
                  </div>
                    <div class="input-field col s12">
                        <input placeholder="" id="title" type="text" name="title" class="form-control">
                        <label for="title">title</label>
                    </div>
                    <div class="input-field col s12">
                        <textarea name="description" id="description" class="materialize-textarea"></textarea>
                        <label for="description">description</label>
                    </div>
                    <div class="input-field col s12">
                        <p class="range-field">
                            <input name="priorityLevel" type="range" id="priorityLevel" min="1" max="5" />
                        </p>
                    </div>
                    <!-- <div class=""> -->
                    <button type="submit" class="btn">edit list</button>
                    <!-- </div> -->
                </form>
            </div>
        </div>
    </div>
</div>
<hr>

