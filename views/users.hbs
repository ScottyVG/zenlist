<div class="container">
    <div class="center-align">
        <h3>create lists proto</h3>
    </div>
    <div class="row">
        <div class="col s6">
            <div class="row input-field">
                <form class="input-field" action="/lists" method="post">
                    <div class="input-field col s12">
                        <input placeholder="" id="title" type="text" name="title" class="form-control">
                        <label for="title">title</label>
                    </div>
                    <div class="input-field col s12">
                        <textarea name="description" id="description" class="materialize-textarea"></textarea>
                        <label for="description">description</label>
                    </div>
                    <div class="input-field col s12">
                        <p class="range-field">
                            <input name="priorityLevel" type="range" id="priorityLevel" min="1" max="5" />
                        </p>
                    </div>
                    <!-- <div class=""> -->
                    <button type="submit" class="btn">Create list</button>
                    <!-- </div> -->
                </form>
            </div>
        </div>
    </div>
</div>
<hr>
